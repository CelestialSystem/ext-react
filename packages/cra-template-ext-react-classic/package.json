{
    "name": "@sencha/cra-template-ext-react-classic",
    "version": "7.1.0",
    "description": "cra-template-ext-react-classic",
    "main": "template.json",
    "files": [
      "template",
      "template.json"
    ],
    "scripts": {
      "start": "react-scripts start",
      "build": "react-scripts build",
      "test": "react-scripts test",
      "eject": "react-scripts eject",
      "clean-files": "rm -rf ./template/public ./template/src && rm -f ./template/.env ./template/.eslintrc  ./template/.nvmrc ./template/.prettierrc ./template/jsconfig.json ./template/.huskyrc ./template/.lintstagedrc ./template/README.md ./template/README_CRA.md",
      "copy-files": "cp -a ./src/. template/src && cp -a ./public/. template/public && cp .env .eslintrc .nvmrc .prettierrc jsconfig.json .huskyrc .lintstagedrc README.md README_CRA.md template/",
      "prepublishOnly": "yarn clean-files && yarn copy-files",
      "fix": "eslint --fix ./src/",
      "lint": "eslint ./src/"
    },
    "dependencies": {
    }
}
