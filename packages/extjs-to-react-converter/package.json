    "@sencha/ext-react-renderercell": "^6.7.0",
    "@sencha/ext-react-transition": "^6.7.0",
    "clipboard": "^2.0.4",
    "prettier-standalone": "^1.3.1-0",
    "prop-types": "^15.6.2",
