    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-react": "^7.0.0",
    "@sencha/ext-react-babel-plugin": "~6.6.4",
    "@sencha/ext-react-webpack-plugin": "~6.6.4",
    "babel-loader": "^8.0.4",
    "cross-env": "^5.2.0",
