{
  "name": "@sencha/ext-react-modern-boilerplate",
  "product": "ext-react",
  "version": "6.7.1",
  "description": "A foundation for React 16 apps built with ExtReact components.",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf build",
    "start": "npm run dev",
    "dev": "webpack-dev-server --env.verbose=no --env.environment=development",
    "prod": "webpack-dev-server --env.verbose=no --env.environment=production",
    "build": "npm run clean && cross-env webpack --env.environment=production",
    "prod2": "cd build && static-server --port 8080",
    "test": "jest",
    "update-snapshots": "jest -u",
    "pretest": "webpack --progress",
    "preupdate-snapshots": "webpack --progress"
  },
  "extbuild": {
    "defaultprofile": "",
    "defaultenvironment": "development",
    "defaultverbose": "no"
  },
  "author": "Sencha, Inc.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sencha/ext-react.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sencha/ext-react/issues"
  },
  "homepage": "https://github.com/sencha/ext-react#readme",
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "@babel/runtime": "^7.3.1",
    "@sencha/ext": "^6.7.0",
    "@sencha/ext-modern": "^6.7.0",
    "@sencha/ext-modern-theme-material": "^6.7.0",
    "@sencha/ext-modern-treegrid": "^6.7.0",
    "@sencha/ext-react": "^7.0.0",
    "@sencha/ext-react-renderercell": "^7.0.0",
    "@sencha/ext-react-transition": "^7.0.0",
    "prop-types": "^15.7.2",
    "react": "~16.8.4",
    "react-dom": "~16.8.4",
    "react-router-dom": "^4.3.1"
  },
  "devDependencies": {
    "@sencha/ext-react-babel-plugin": "^6.7.0",
    "@sencha/ext-webpack-plugin": "^7.0.0",
    "@babel/core": "^7.3.4",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-proposal-decorators": "^7.3.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.2.0",
    "@babel/plugin-proposal-function-sent": "^7.2.0",
    "@babel/plugin-proposal-json-strings": "^7.2.0",
    "@babel/plugin-proposal-numeric-separator": "^7.2.0",
    "@babel/plugin-proposal-throw-expressions": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-syntax-import-meta": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.5",
    "cross-env": "^5.2.0",
    "html-webpack-plugin": "^3.2.0",
    "style-loader": "^0.23.1",
    "css-loader": "^2.1.0",
    "portfinder": "^1.0.20",
    "react-hot-loader": "^4.7.1",
    "rimraf": "^2.6.3",
    "webpack": "^4.29.5",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "setupFiles": [
      "./node_modules/@sencha/ext-react/jest/setup.js",
      "./build/ext-react/ext.js"
    ],
    "unmockedModulePathPatterns": [
      "react",
      "react-dom",
      "react-addons-test-utils",
      "enzyme"
    ]
  }
}
