    "prop-types": "~15.7.2",

    "react": "~16.8.4",
    "@hot-loader/react-dom":"~16.8.4",
    "react-redux": "~6.0.1",
    "react-router-dom": "~5.0.0",
    "redux": "~4.0.1"
  },
  "devDependencies": {
    "@sencha/ext-react-babel-plugin": "^6.7.1",
    "@sencha/ext-react-webpack-plugin": "~6.7.1",

