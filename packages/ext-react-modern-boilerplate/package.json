{
  "name": "@sencha/ext-react-modern-boilerplate",
  "product": "ext-react",
  "version": "6.7.1",
  "description": "A foundation for React 16 apps built with ExtReact components.",
  "main": "index.js",
  "scripts": {
    "start": "npm run dev",
    "clean": "rimraf build",
    "dev": "webpack-dev-server --env.browser='yes'  --env.verbose='no'",
    "build": "npm run clean && cross-env webpack --env.environment='production' --env.treeshake='yes'"
